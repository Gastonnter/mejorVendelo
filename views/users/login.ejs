<%- include("../partials/head", {pageTitle: "Ingresar", stylesheet: "login.css"}) %>


    <%- include("../partials/header", {logueado: undefined ,tipo: undefined}) %>
    
    
     
    <main>
        <form action="/users/login" method="POST">
            
            <p>
                <label></label>
                <input class="inputFormulario" type="text" name="email" required placeholder="Email de usuario" id="emailLogin">
                <div class="error" id="emailError"></div>
            </p>

            <p>
                <label></label>
                <input class="inputFormulario" type="password" name="password" required placeholder="Contraseña" id="passwordLogin">
                <div class="error" id="passwordError"></div>
            </p>

<% if (typeof error !== 'undefined') { %>
    <div class="alert"><%= error %></div>
  <% } %>


            <button class=“botonEnviar” type="submit" value="ENVIAR">ENVIAR</button>
           
                 

        </form>

    </main>
    </header>
    <script src ="/js/userValidationLogin.js" defer> </script>
    <%- include("../partials/footer") %>

